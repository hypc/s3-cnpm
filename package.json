{
  "name": "@hyu/s3-cnpm",
  "version": "1.0.0",
  "description": "s3 wrapper for cnpm",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json --fix src/**/*.ts",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyunrealshadow/s3-cnpm.git"
  },
  "keywords": [
    "s3",
    "storage",
    "cnpm"
  ],
  "author": "UnrealShadow <unrealshadow@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hyunrealshadow/s3-cnpm/issues"
  },
  "homepage": "https://github.com/hyunrealshadow/s3-cnpm#readme",
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "dotenv": "^6.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "aws-sdk": "^2.384.0"
  }
}
